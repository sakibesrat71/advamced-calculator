{
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "scripts": {
    "test": "jest",
    "test:unit": "jest --selectProjects unit",
    "test:integration": "jest --selectProjects integration"
  },
  "jest": {
    "projects": [
      {
        "displayName": "unit",
        "testMatch": ["**/*.test.js"]
      },
      {
        "displayName": "integration",
        "testMatch": ["**/*.integration.js"]
      }
    ]
  }
}
